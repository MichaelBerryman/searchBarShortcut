{
    "manifest_version": 2,
    "name": "searchBarShortcut",
    "description": "Use a keyboard shortcut to cycle the focus through the sites search bars",
    "version": "1.0",
    "homepage_url": "https://github.com/MichaelBerryman/searchBarShortcut",

    "icons": {
        "48": "icons/icon-48.png"
        "96": "icons/icon-96.png"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
    }]
}